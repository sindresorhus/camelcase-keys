		"camelcase",
		"camel-case",
		"camel",
		"case",
		"dash",
		"hyphen",
		"dot",
		"underscore",
		"separator",
		"string",
		"text",
		"convert",
		"pascalcase",
		"pascal-case",
		"deep",
		"recurse",
		"recursive"
	],
	"dependencies": {
		"camelcase": "^8.0.0",
		"map-obj": "5.0.0",
		"quick-lru": "^6.1.1",
		"type-fest": "^4.2.0"
	},
	"devDependencies": {
		"ava": "^5.3.1",
		"matcha": "^0.7.0",
		"tsd": "^0.28.1",
		"xo": "^0.56.0"
	},
	"xo": {
		"overrides": [
			{
				"files": "bench/bench.js",
				"rules": {
					"import/no-unresolved": "off"
				}
			}
		]
	}
