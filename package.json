{
	"name": "camelcase-keys",
	"version": "10.0.0",
	"description": "Convert object keys to camel case",
	"license": "MIT",
	"repository": "sindresorhus/camelcase-keys",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": {
		"types": "./index.d.ts",
		"default": "./index.js"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"test": "xo && ava && tsd",
